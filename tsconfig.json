{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "incremental": true,
    "baseUrl": ".",
    "paths": {}
  },
  "include": [ "**/*.ts", "**/*.js"],
  "exclude": []
}